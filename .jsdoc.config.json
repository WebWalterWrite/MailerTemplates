{
	"plugins": [],
	"recurseDepth": 10,
	"opts": {
		"recurse": true,
		"destination": "./docs/"
	},
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.mjs(doc|x)?$",
		"excludePattern": "node_modules"
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
